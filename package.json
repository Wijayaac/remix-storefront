{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "remix setup && patch-package",
    "prebuild": "patch-package",
    "build": "npm run codegen && npm run build:css && remix build",
    "dev": "pm2-dev pm2.config.js",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss --output ./app/styles/tailwind.css --watch --postcss",
    "build:css": "tailwindcss --output ./app/styles/tailwind.css --minify --postcss",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.yml"
  },
  "dependencies": {
    "@headlessui/react": "1.4.1",
    "@heroicons/react": "1.0.4",
    "@mcansh/fetcher": "0.2.2",
    "@netlify/functions": "0.7.2",
    "@remix-run/netlify": "0.19.3",
    "@remix-run/node": "0.19.3",
    "@remix-run/react": "0.19.3",
    "date-fns": "2.25.0",
    "graphql": "15.6.1",
    "graphql-request": "3.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.0.0-beta.6",
    "remix": "0.19.3",
    "remix-utils": "1.0.2",
    "type-fest": "2.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.1",
    "@graphql-codegen/typescript": "2.2.4",
    "@graphql-codegen/typescript-graphql-request": "4.1.6",
    "@graphql-codegen/typescript-operations": "2.1.8",
    "@remix-run/dev": "0.19.3",
    "@tailwindcss/aspect-ratio": "0.3.0",
    "@tailwindcss/forms": "0.3.4",
    "@tailwindcss/typography": "0.4.1",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "autoprefixer": "10.3.7",
    "netlify-cli": "6.13.2",
    "patch-package": "6.4.7",
    "pm2": "5.1.2",
    "postcss-100vh-fix": "1.0.2",
    "tailwindcss": "2.2.17",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
