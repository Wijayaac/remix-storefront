{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "remix setup",
    "build": "npm run codegen && npm run build:css && remix build",
    "predev": "npm run codegen",
    "dev": "pm2-dev pm2.config.js",
    "dev:css": "tailwindcss --output ./app/styles/tailwind.css --watch --postcss",
    "build:css": "tailwindcss --output ./app/styles/tailwind.css --minify --postcss",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.yml",
    "clean": "del .cache .netlify app/graphql/index.ts app/styles/tailwind.css public/build netlify/functions/server/build",
    "lint": "eslint --cache --fix --ignore-path .gitignore .",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@headlessui/react": "1.7.5",
    "@heroicons/react": "2.0.13",
    "@mcansh/fetcher": "0.2.2",
    "@netlify/functions": "1.3.0",
    "@remix-run/netlify": "1.8.2",
    "@remix-run/node": "1.8.2",
    "@remix-run/react": "1.8.2",
    "date-fns": "2.29.3",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix": "1.8.2",
    "type-fest": "3.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/typescript": "2.8.5",
    "@graphql-codegen/typescript-graphql-request": "4.5.8",
    "@graphql-codegen/typescript-operations": "2.5.10",
    "@remix-run/dev": "1.8.2",
    "@remix-run/eslint-config": "1.8.2",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.8",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "10.4.13",
    "del-cli": "5.0.0",
    "eslint": "8.29.0",
    "eslint-plugin-prefer-let": "3.0.1",
    "eslint-plugin-remix-react-routes": "1.0.4",
    "netlify-cli": "12.2.10",
    "pm2": "5.2.2",
    "postcss-100vh-fix": "1.0.2",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
